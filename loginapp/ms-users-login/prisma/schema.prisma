datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id         Int      @id @default(autoincrement())
  email      String   @unique
  password   String
  uuid       String   @unique @default(uuid())
  createdAt  DateTime @default(now())
  deletedAt  DateTime? // nullable
  modifiedAt DateTime? // nullable
}